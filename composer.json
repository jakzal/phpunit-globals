{
    "name": "zalas/phpunit-globals",
    "description": "Allows to use annotations to define global variables in PHPUnit test cases.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Zalas",
            "email": "jakub@zalas.pl"
        }
    ],
    "require": {
        "php": "^7.2",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Zalas\\PHPUnit\\Globals\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zalas\\PHPUnit\\Globals\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
